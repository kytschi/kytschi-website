            PRINT "Want to check it out?! Click below..."
            LINE BREAK
            LINE BREAK
            LINK "https://talkopedia.com", "Check it out live", "nav-link-img", "_blank"
                IMAGE "/kytschi/imgs/best_viewed_with_eyes.gif", "Check it out live"
            LINK CLOSE
        SWRITE CLOSE
    DIV CLOSE
