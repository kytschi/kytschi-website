        DIV "card"
            DIV "card-body"
                IMAGE "/kytschi/imgs/portfolio/talkopedia.png", "talkopedia"
            DIV CLOSE
            DIV "card-footer"
                SWRITE
                    PRINT "Talkopedia"
                SWRITE CLOSE
            DIV CLOSE
        DIV CLOSE
    DIV CLOSE
    DIV "table-text"
        SWRITE
            PRINT "A port of a WordPress website into a CMS I wrote called Tengu that is more SEO focused."
        SWRITE CLOSE
        SWRITE
            PRINT "Talkopedia's main focus is to offer reviews of various things that are designed to help better people's lives."
        SWRITE CLOSE
        SWRITE
            PRINT "Key features", "text-heading"
        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Content spinner"
        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Persona management for blog posts"
        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Keyword anayliser"
        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Auto-theme switcher at set dates, Christmas for example"
        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Marketing campaigns"
        SWRITE CLOSE
    DIV CLOSE
DIV CLOSE
