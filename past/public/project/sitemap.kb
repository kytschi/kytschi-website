LANG "en"
HEAD
	LOAD "_ROOT/project/shared/head"
	NAME "sitemap | kytschi"
HEAD CLOSE
BODY
	LOAD "_ROOT/project/shared/header"
	MAIN
		HEADING 4
			PRINT "Sitemap"
		HEADING CLOSE
		DIV "table"
			DIV "table-img mobile-margin"
				DIV "card"
					DIV "card-body"
						IMAGE "/kytschi/imgs/cat-lets-go.gif", "Meeeeoooow!"
					DIV CLOSE
					DIV "card-footer"
						SWRITE
							PRINT "Just list the site"
							LINE BREAK
							PRINT "will you!"
						SWRITE CLOSE
					DIV CLOSE
				DIV CLOSE
			DIV CLOSE
			DIV "table-text no-mobile-margin"
				IMAGE "/kytschi/imgs/arrow.png", "arrow"
			DIV CLOSE
		DIV CLOSE
		SWRITE
			LINK "/","Go to the homepage"
				HEADING 4
					PRINT "Home"
				HEADING CLOSE
			LINK CLOSE
		SWRITE CLOSE
		SWRITE
			LINK "/contact-me","Go to the contact page"
				HEADING 4
					PRINT "Contact me"
				HEADING CLOSE
			LINK CLOSE
		SWRITE CLOSE
		SWRITE
			LINK "/blog-land","Go to the blog page"
				HEADING 4
					PRINT "Blog"
				HEADING CLOSE
			LINK CLOSE
		SWRITE CLOSE
		SWRITE
			LINK "/blog-land/man-writing-compiler-seeks-woman","Man writing own compiler seeks Woman"
				HEADING 4
					PRINT "Man writing own compiler seeks Woman"
				HEADING CLOSE
			LINK CLOSE
		SWRITE CLOSE
		SWRITE
			LINK "/blog-land/how-to-deal-with-job-rejection","How to handle job application rejection hell!"
				HEADING 4
					PRINT "How to handle job seeking rejection hell!"
				HEADING CLOSE
			LINK CLOSE
		SWRITE CLOSE
		SWRITE
			LINK "/blog-land/if-job-hunting-was-a-meme-it-would-be-this","If job hunting was a meme it would be this"
				HEADING 4
					PRINT "If job hunting was a meme it would be this"
				HEADING CLOSE
			LINK CLOSE
		SWRITE CLOSE
	MAIN CLOSE
	LOAD "_ROOT/project/shared/footer"
BODY CLOSE
END
