LANG "en"
HEAD
	LOAD "_ROOT/project/shared/head"
	NAME "sitemap | kytschi"
HEAD CLOSE
BODY
	LOAD "_ROOT/project/shared/header"
	MAIN
		HEADING 3
			PRINT "Sitemap"
		HEADING CLOSE
		DIV "table"
			DIV "table-img mobile-margin"
				DIV "card"
					DIV "card-body"
						IMAGE "/kytschi/imgs/cat-lets-go.gif", "Meeeeoooow!"
					DIV CLOSE
					DIV "card-footer"
						SWRITE
							PRINT "Just list the site"
							LINE BREAK
							PRINT "will you!"
						SWRITE CLOSE
					DIV CLOSE
				DIV CLOSE
			DIV CLOSE
			DIV "table-text no-mobile-margin"
				IMAGE "/kytschi/imgs/arrow.png", "arrow"
			DIV CLOSE
		DIV CLOSE
		SWRITE
			LINK "/","Go to the homepage"
				HEADING 4
					PRINT "Home"
				HEADING CLOSE
			LINK CLOSE
		SWRITE CLOSE
		SWRITE
			LINK "/contact-me","Go to the contact page"
				HEADING 4
					PRINT "Contact me"
				HEADING CLOSE
			LINK CLOSE
		SWRITE CLOSE
		SWRITE
			LINK "/blog-land","Go to the blog page"
				HEADING 4
					PRINT "Blog"
				HEADING CLOSE
			LINK CLOSE
		SWRITE CLOSE
	MAIN CLOSE
	LOAD "_ROOT/project/shared/footer"
BODY CLOSE
END
