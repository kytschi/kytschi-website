        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Product management with sync to their stock control system"
        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Customer management with links to their dental finder apps"
        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Credit notes system"
        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Invoice management"
        SWRITE CLOSE
        SWRITE "bullet-item"
            PRINT "Customer import system with their old software called Evident"
        SWRITE CLOSE
    DIV CLOSE
